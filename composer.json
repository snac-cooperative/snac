{

    "require": {
        "php": "~7.0",
        "twig/twig": "^2.0",
        "monolog/monolog": "^1.18",
        "elasticsearch/elasticsearch": "7.11",
        "phpmailer/phpmailer": "^6.0",
        "snac/eac-validator": "*",
        "snac/snac-public-web": "*",
        "snac/snac-ead-parser": "*",
        "graphaware/neo4j-php-client": "^4.8",
        "jasny/twig-extensions": "^1.3",
        "soundasleep/html2text": "~0.5",
        "league/oauth2-client": "^2.4",
        "league/oauth2-google": "^3.0",
        "twig/string-extra": "^3.0",
        "snac/ark-manager": "^1.0"
    },
    "require-dev": {

        "phpunit/phpunit": ">=5.0",
        "phpunit/php-invoker": "*",
        "phpunit/dbunit": ">=1.2",
        "phpunit/phpunit-selenium": ">=1.2",
        "behat/behat": "*",
        "deployer/deployer": "^7.4"
    },
    "autoload": {
        "psr-4": {
            "snac\\": "src/snac/"
        }
    },
    "repositories": [{
            "type": "vcs",
            "url": "https://github.com/snac-cooperative/snac-laravel.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/snac-cooperative/snac-public-web.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/snac-cooperative/ead-parser.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/snac-cooperative/ark-manager.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/snac-cooperative/eac-validator.git"
        }

    ],
        "config": {
            "process-timeout": 600,
            "preferred-install": "dist",
            "github-protocols": ["https"],
        "allow-plugins": {
            "php-http/discovery": true
        }
        }
}
